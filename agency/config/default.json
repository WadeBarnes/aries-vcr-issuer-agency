{
  "host": "localhost",
  "port": "PORT",
  "public": "../public/",
  "paginate": {
    "default": 10,
    "max": 50
  },
  "authentication": {
    "adminApiKey": "CONTROLLER_ADMIN_API_KEY"
  },
  "mongodb": {
    "host": "DB_SERVICE",
    "db": "DB_NAME",
    "port": "DB_PORT",
    "user": "DB_USER",
    "password": "DB_PASSWORD"
  },
  "agent": {
    "adminUrl": "AGENCY_ADMIN_URL",
    "headers": {
      "x-api-key": "AGENCY_ADMIN_API_KEY"
    },
    "credExTimeout": "CRED_EX_TIMEOUT",
    "transactionTimeout": "TRANSACTION_TIMEOUT"
  },
  "aries-vcr": {
    "adminUrl": "REGISTRY_ADMIN_URL",
    "headers": {
      "x-api-key": "REGISTRY_ADMIN_API_KEY"
    },
    "alias": "REGISTRY_ALIAS"
  },
  "endorser": {
    "adminUrl": "ENDORSER_ADMIN_URL",
    "headers": {
      "x-api-key": "ENDORSER_ADMIN_API_KEY"
    },
    "alias": "ENDORSER_ALIAS"
  },
  "issuer": {
    "taa_method": "service_agreement"
  },
  "logging": {
    "logLevel": "LOG_LEVEL"
  },
  "defaultWebhookHosts": [
    "AGENCY_WEBHOOK_URL"
  ]
}
